<plist version="1.0">
<dict>
	<key>sample enemy type</key>
	<dict>
		<!-- MARK: stats -->
		<key>str</key> <integer>10</integer>
		<key>dex</key> <integer>10</integer>
		<key>cun</key> <integer>10</integer>
		<key>wis</key> <integer>10</integer>
		<key>end</key> <integer>10</integer>
		<!-- MARK: equipment -->
		<key>weapon</key> <string>sword</string>
		<key>weapon material</key> <string>steel</string>
		<key>armor</key> <string>heavy armor</string>
