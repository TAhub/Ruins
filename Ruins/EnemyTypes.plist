		<key>racial group</key> <string>undead</string>
		<key>appearance group</key> <string>zombie</string>
		<!-- MARK: generation data -->
		<key>level</key> <integer>1</integer>
		<key>keywords</key>
		<array>
			<string>poison</string>
			<string>stun</string>
			<string>shake</string>
		</array>
	</dict>
